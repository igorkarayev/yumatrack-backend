{
  "name": "yumatrack",
  "version": "0.0.1",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "commit": "npx git-cz",
    "build:watch": "webpack --watch --mode development --config webpack.config.js",
    "build": "webpack --mode development --config webpack.config.js",
    "local": "npm-run-all -s migration:run start:server",
    "start:server": "npm-run-all -p start:watch build:watch",
    "start:watch": "nodemon ./dist/app.js",
    "start": "node ./dist/app.js",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/ormconfig.migration.ts",
    "migration:create": "npm run typeorm migration:create -- -d src/database/migrations -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "docs:generate": "speccy resolve  src/docs/main.yml -o src/docs/openapi.yml",
    "seed": "ts-node -r tsconfig-paths/register -r dotenv/config node_modules/typeorm-seeding/dist/cli.js seed --configName ./src/ormconfig.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "npm run lint"
    ],
    "src/**/*.{js,ts}": [
      "npm run build"
    ],
    "lambdas/**/*.{js,ts}": [
      "npm run build:lambdas"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@coinify/currency": "^1.3.0",
    "@types/qrcode": "^1.3.4",
    "@types/speakeasy": "^2.0.5",
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.660.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "big.js": "^5.2.2",
    "cache-manager": "^3.3.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "emitter-listener": "^1.1.2",
    "express": "^4.17.1",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "mustache": "^4.0.1",
    "p-debounce": "^2.1.0",
    "pg": "^7.18.2",
    "pg-native": "^3.0.0",
    "pluralize": "^8.0.0",
    "qrcode": "^1.4.4",
    "random-js": "^2.1.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "source-map-loader": "^0.2.4",
    "speakeasy": "^2.0.0",
    "tslib": "1.11.1",
    "typeorm": "^0.2.24",
    "typeorm-seeding": "^1.3.0",
    "url-pattern": "^1.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.50",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/big.js": "^4.0.5",
    "@types/cache-manager": "^2.10.2",
    "@types/cls-hooked": "^4.3.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.11",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mustache": "^4.0.1",
    "@types/node": "^13.9.0",
    "@types/p-debounce": "^1.0.1",
    "@types/pg": "^7.14.3",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.1",
    "@types/randomstring": "^1.1.6",
    "@types/triple-beam": "^1.3.0",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-webpack": "^5.3.1",
    "source-map-support": "^0.5.16",
    "speccy": "^0.11.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.0.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
